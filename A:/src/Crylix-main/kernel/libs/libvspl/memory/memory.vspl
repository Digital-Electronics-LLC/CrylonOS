//the memory library of the official vspl library

/* some vars */

//for malloc
int blocks = 32768;
int block_size = 64;

function peek(int address)
{
  save(A, address[address]);
  return(A);
  xor(A, A);
}

function poke(int address, int value)
{
  savei(A, value);
  store(A, address);
  xor(A, A);
}


function alloc(size)
{
  nop();
}

